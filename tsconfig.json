{
    "compilerOptions": {
        "outDir": "./build/dist/",
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "ES6",
        "jsx": "react",
        "allowJs": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true
    },
    "include": [
      "src/static"
    ]
}
